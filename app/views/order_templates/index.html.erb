<p id="notice"><%= notice %></p>

<h1>Listing Order Templates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_templates.each do |order_template| %>
      <tr>
        <td><%= link_to 'Show', vendor_order_template %></td>
        <td><%= link_to 'Edit', edit_vendor_order_template_path(order_template) %></td>
        <td><%= link_to 'Destroy', vendor_order_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order template', new_vendor_order_template_path %>
